{
  "expo": {
    "name": "MyBoard",
    "slug": "MyBoard",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.myboardapp.myboard",
      "buildNumber": "11",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "We use your photo to set your profile picture.",
        "NSCameraUsageDescription": "Allow camera to take a profile photo.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1cd1ec7c-39cf-4686-bc36-3204247d6cbe"
      }
    }
  }
}
